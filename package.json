{
  "name": "tempo-launcher",
  "version": "0.2.4-beta1",
  "description": "TempoLauncher allow you to find and launch apps on your PC.",
  "license": "MIT",
  "author": {
    "name": "Roman Konkin",
    "email": "sly.feafarot@gmail.com"
  },
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "publish-tool": "powershell -File .\\src\\icon-extractor-tool\\publish.ps1",
    "postinstall": "electron-builder install-app-deps",
    "test": "jest",
    "test-w": "jest --watch -o"
  },
  "build": {
    "appId": "tempo-launcher",
    "productName": "TempoLauncher",
    "copyright": "Copyright Â© 2019 ${author}",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "extraResources": [
      {
        "from": "src/icon-extarctor-tool/bin/Release/netcoreapp3.0/win10-x64/publish",
        "to": "tools/icon-extractor",
        "filter": [
          "**/*"
        ]
      },
      "static"
    ]
  },
  "electronWebpack": {
    "title": "TempoLauncher",
    "whiteListedModules": [
      "@material-ui/styles",
      "@material-ui/core"
    ],
    "renderer": {
      "webpackConfig": "webpack.customizations.js"
    },
    "main": {
      "webpackConfig": "webpack.customizations.js"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.4.3",
    "@material-ui/icons": "^4.4.3",
    "@material-ui/styles": "^4.4.3",
    "@types/auto-launch": "^5.0.1",
    "@types/lodash": "^4.14.139",
    "auto-launch": "^5.0.5",
    "date-fns": "^2.3.0",
    "electron-log": "^3.0.8",
    "electron-store": "^5.0.0",
    "electron-unhandled": "^3.0.0",
    "globby": "^10.0.1",
    "lodash": "^4.17.15",
    "mathjs": "^6.2.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rxjs": "^6.5.3",
    "source-map-support": "^0.5.13",
    "typeface-roboto": "^0.0.75"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^2.0.1",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/mathjs": "^6.0.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "electron": "6.0.0-beta.13",
    "electron-builder": "^21.2.0",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "ts-jest": "^24.1.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.20.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.6.3",
    "typescript-tslint-plugin": "^0.5.4",
    "webpack": "~4.35.3"
  }
}
